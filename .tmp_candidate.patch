+from difflib import SequenceMatcher

def fuzzy_label_match(query: str, labels: list[str], synonyms: dict[str, str]) -> str:
    q = query.lower().strip()
    if q in synonyms:
        return synonyms[q]

    best = ""
    score = 0.0
    for label in labels:
        current = SequenceMatcher(None, q, label.lower()).ratio()
        if current > score:
            score = current
            best = label
    return best
